<?xml version="1.0" encoding="UTF-8"?>

<namespace xmlns="http://www.mulesoft.org/schema/mule/tooling.attributes" xmlns:mule="http://www.mulesoft.org/schema/mule/core" url="http://www.mulesoft.org/schema/mule/elastic" prefix="elastic">
  <global-cloud-connector versions="[3.5.0,8.0.0]" caption="Elastic" description="" icon="" localId="parentConfig" image="" abstract="true" extends="http://www.mulesoft.org/schema/mule/elastic/abstractElasticConnectorGeneric"></global-cloud-connector>
  <global-cloud-connector versions="[3.5.0,8.0.0]" caption="Elastic: Configuration" description="Global Configuration configuration information." icon="icons/small/elastic-connector-24x16.png" localId="config" image="icons/large/elastic-connector-48x32.png" abstract="false" extends="http://www.mulesoft.org/schema/mule/elastic/parentConfig" connectivityTesting="off" metaData="static" metaDataKeyDisplay="DISPLAY_NAME" supportsUserDefinedMetaData="false">
    <attribute-category caption="General" description="Elastic configuration properties.">
      <group id="elasticGenericProperties" caption="Basic Settings">
        <name name="name" caption="Name" description="Give a name to this configuration so it can be later referenced by config-ref." required="true"></name>
      </group>
      <group id="general" caption="General">
        <string defaultValue="Hello" name="greeting" caption="Greeting" description="Greeting message." supportsExpressions="false" required="false" javaType="java.lang.String"></string>
        <string defaultValue="How are you?" name="reply" caption="Reply" description="Reply message." supportsExpressions="false" required="false" javaType="java.lang.String"></string>
      </group>
    </attribute-category>
  </global-cloud-connector>
  <cloud-connector caption="Abstractelasticconnectorgeneric" description="Interact with Elastic." icon="icons/small/elastic-connector-24x16.png" localId="abstractElasticConnectorGeneric" image="icons/large/elastic-connector-48x32.png" abstract="true">
    <attribute-category caption="General" description="General.">
      <group id="abstractElasticConnectorGeneric" caption="Basic Settings">
        <globalRef requiredType="http://www.mulesoft.org/schema/mule/elastic/config" name="config-ref" caption="Connector Configuration" description="Specify which configuration to use for this invocation." required="true"></globalRef>
      </group>
    </attribute-category>
  </cloud-connector>
  <cloud-connector versions="[3.5.0,8.0.0]" aliasId="org.mule.tooling.ui.modules.core.pattern.elastic" caption="Elastic" description="" icon="icons/small/elastic-connector-24x16.png" localId="elastic-connector" image="icons/large/elastic-connector-48x32.png" extends="http://www.mulesoft.org/schema/mule/elastic/abstractElasticConnectorGeneric">
    <attribute-category caption="General" description="General.">
      <group id="elasticConnectorGeneric" caption="Basic Settings">
        <modeSwitch alwaysCombo="true" name="Elastic operations to execute" caption="Operation" description="Operation.">
          <no-operation abstractElement="http://www.mulesoft.org/schema/mule/elastic/abstractElasticConnectorGeneric" versions="[3.5.0,8.0.0]" connector-name="processor"></no-operation>
          <mode modeId="http://www.mulesoft.org/schema/mule/elastic/greet" modeLabel="Greet"></mode>
        </modeSwitch>
      </group>
    </attribute-category>
  </cloud-connector>
  <cloud-connector returnType="java.lang.String" caption="Greet" icon="icons/small/elastic-connector-24x16.png" localId="greet" image="icons/large/elastic-connector-48x32.png" abstract="true" extends="http://www.mulesoft.org/schema/mule/elastic/abstractElasticConnectorGeneric" metaData="static">
    <attribute-category caption="General" description="General.">
      <group id="general" caption="General">
        <string name="friend" caption="Friend" description="Name to be used to generate a greeting message." required="true" javaType="java.lang.String"></string>
      </group>
    </attribute-category>
  </cloud-connector>
